     "OMRS_CONFIG_CONNECTION_USERNAME":"openmrs",
     "OMRS_CONFIG_CONNECTION_PASSWORD":"dev_password_only",
     "OMRS_DEV_DEBUG_PORT":"1044",
     "OMRS_JAVA_MEMORY_OPTS":"-Xmx8192m -Xms4096m -XX:NewSize=512m",
     "ISANTEPLUS_VERSION": "v2.3.4"
  }
