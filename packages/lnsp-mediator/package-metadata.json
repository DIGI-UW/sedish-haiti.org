{
  "id": "lnsp-mediator",
  "name": "lnsp-mediator",
  "description": "Repository for emulating XDS document storage for the Lab Workflow",
  "type": "infrastructure",
  "version": "0.0.0",
  "dependencies": [],
  "environmentVariables": { 
    "MONGO_IMAGE": "mongo:7.0.12",
    "MONGO_1_PLACEMENT": "node-1",
    "MONGO_2_PLACEMENT": "node-2",
    "MONGO_3_PLACEMENT": "node-3",
    "MONGO_SET_COUNT": "1",
    "MONGO_CPU_LIMIT": "0",
    "MONGO_CPU_RESERVE": "0.05",
    "MONGO_MEMORY_LIMIT": "2G",
    "MONGO_MEMORY_RESERVE": "500M",
    "MONGO_INITDB_ROOT_PASSWORD": "change-admin-password",
    "MONGO_INITDB_ROOT_USERNAME": "admin",
    "DB_PORT": "27017",
    "DB_HOST": "lnsp-mongo-1",
    "DB_NAME": "nest",
    "MONGO_URL": "mongodb://lnsp-mongo-1:27017/nest",
    "MONGO_ATNAURL": "mongodb://lnsp-mongo-1:27017/nest",
    "AWAIT_HELPER_IMAGE": "jembi/await-helper:1.0.1",
    "LNSP_DB_EXISTS": "false",
    "LNSP_RUN_MIGRATIONS": "true"
  }
}

