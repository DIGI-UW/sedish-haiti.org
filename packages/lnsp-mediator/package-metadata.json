{
  "id": "lnsp-mediator",
  "name": "lnsp-mediator",
  "description": "Repository for emulating XDS document storage for the Lab Workflow",
  "type": "infrastructure",
  "version": "0.0.0",
  "dependencies": [],
  "environmentVariables": { 
    "MONGO_IMAGE": "mongo:7.0.12",
    "MONGO_1_PLACEMENT": "node-1",
